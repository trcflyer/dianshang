<!--pages/logistics/logistics.wxml-->
 <view class='log-box' wx:for="{{carrieInfo.Traces}}" wx:key="item" wx:for-index="idx" wx:for-item="item">
  <view class="log-list">
    <view class="log-detail">
    <view class='log-left'>{{item.AcceptStation}}</view>
      <view class='log-right'>
        <text style='font-size:26rpx;'>{{item.date}}</text>
        <text style='font-size:24rpx;padding-left:12rpx;'>{{item.time}}</text> 
      </view>
    </view>
  </view>
  
</view> 
